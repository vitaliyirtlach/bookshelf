{
  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": true,       
    "allowSyntheticDefaultImports": true,       
    "target": "es6",                       
    "module": "commonjs",                     
    "jsx": "react", 
    "outDir": "./dist",  
    "strict": true,  
    "esModuleInterop": true, 
    "moduleResolution": "node",
    "skipLibCheck": true,                 
    "forceConsistentCasingInFileNames": true
  }
}
